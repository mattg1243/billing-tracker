  "engines": {
    "node": "14.17.4"
  },
  "private": true,
  "scripts": {
    "start": "node ./dist/app.js",
    "tsc": "tsc",
    "devstart": "tsc-watch --noClear -p ./tsconfig.json --onSuccess \"node ./dist/app.js\"",
    "build": "cd client && npm run build",
    "serverstart": "DEBUG=exp-billing:* npm run devstart",
    "test": "mocha --require mocha-suppress-logs -r ts-node/register --exit -t 5000 'test/*.test.ts'",
