    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "express-session": "^1.17.2",
    "express-validator": "^6.14.0",
    "helmet": "^4.6.0",
    "http-errors": "~1.6.3",
    "jquery-validation": "^1.19.3",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongodb": "^4.6.0",
    "mongoose": "^6.0.5",
    "morgan": "~1.9.1",
    "nodemailer": "^6.7.2",
    "passport": "^0.4.1",
    "passport-local-mongoose": "^6.1.0",
