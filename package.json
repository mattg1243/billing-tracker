{
  "name": "exp-billing",
  "version": "0.0.0",
  "engines": {
    "node": "14.17.4"
  },
  "private": true,
  "scripts": {
    "start": "node ./dist/app.js",
    "tsc": "tsc",
    "postinstall": "npm run tsc",
    "devstart": "nodemon ./src/app.ts",
    "build": "cd client && npm run build",
    "serverstart": "DEBUG=exp-billing:* npm run devstart",
    "test": "mocha --require mocha-suppress-logs --exit -t 5000 src/test",
    "buildpush": "cd client && npm run build && cd .. && git add . && git commit -m 'new react build' && git push origin react && git push heroku react:main"
  },
