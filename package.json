    "mocha-suppress-logs": "^0.3.1",
    "node-mocks-http": "^1.11.0",
    "nodemon": "^2.0.12",
    "sinon": "^14.0.0",
    "supertest": "^6.2.4",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  }
